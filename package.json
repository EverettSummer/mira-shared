{
   "name": "@irohalab/mira-shared",
   "version": "2.0.0",
   "description": "Shared components for mira projects",
   "main": "index.js",
   "repository": {
      "type": "git",
      "url": "https://github.com/irohalab/mira-shared.git"
   },
   "scripts": {
      "test": "$(npm bin)/ava",
      "clean:dist": "rimraf dist/*",
      "build": "npm run clean:dist && tsc && cp package.json dist",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
   },
   "keywords": [],
   "author": "Everett Summer",
   "license": "Apache-2.0",
   "dependencies": {
      "@mikro-orm/core": "^5.1.3",
      "@mikro-orm/postgresql": "^5.1.3",
      "@sentry/node": "^6.19.2",
      "amqplib": "^0.7.1",
      "inversify": "^5.0.5",
      "inversify-binding-decorators": "^4.0.0",
      "inversify-logger-middleware": "^3.1.0",
      "pg": "^8.4.0",
      "pino": "^7.6.2",
      "reflect-metadata": "^0.1.10",
      "uuid": "^8.3.2"
   },
   "devDependencies": {
      "@types/amqplib": "^0.8.0",
      "@types/node": "^16.3.3",
      "@types/supertest": "^2.0.11",
      "ava": "^3.15.0",
      "cross-env": "^7.0.3",
      "rimraf": "^3.0.2",
      "supertest": "^6.1.4",
      "ts-node": "3.3.0",
      "tslint": "^6.1.3",
      "typescript": "^4.3.5"
   },
   "engines": {
      "node": ">=16.0.0"
   },
   "ava": {
      "extensions": [
         "ts"
      ],
      "files": [
         "src/services/*.spec.ts"
      ],
      "require": [
         "ts-node/register"
      ],
      "verbose": true
   },
   "publishConfig": {
      "access": "public",
      "registry": "https://registry.npmjs.org/"
   }
}
